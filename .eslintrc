{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "airbnb-base"
  ],
  "plugins": [
    "babel",
    "react",
    "promise"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "DEV": false,
    "TEST": false,
    "PROD": false,
    "COVERAGE": false
  },
  "rules": {
    "max-len": [2, 120, 2],
    "no-param-reassign": 0,
    "func-names": 0,
    "array-callback-return": 0,
  }
}
